{
  "name": "edge-craft",
  "version": "0.1.0",
  "description": "WebGL-based RTS game engine supporting classic RTS map formats",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode production",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --passWithNoTests",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit --strict --noUnusedLocals --noUnusedParameters",
    "validate": "npm run validate:licenses && npm run validate:credits",
    "validate:licenses": "node scripts/validation/PackageLicenseValidator.cjs",
    "validate:credits": "node scripts/validation/AssetCreditsValidator.cjs",
    "optimize": "vite optimize",
    "clean": "rm -rf dist .vite node_modules/.vite",
    "install:hooks": "node scripts/hooks/install-hooks.js",
    "uninstall:hooks": "node scripts/hooks/uninstall-hooks.js",
    "precommit": "bash scripts/hooks/pre-commit"
  },
  "dependencies": {
    "@babylonjs/core": "^7.0.0",
    "@babylonjs/loaders": "^7.0.0",
    "@types/lzma-native": "^4.0.4",
    "@types/pako": "^2.0.4",
    "lzma-native": "^8.0.6",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.4",
    "seek-bzip": "^2.0.0",
    "wc3maptranslator": "^4.0.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.5.1",
    "prettier": "^3.6.2",
    "terser": "^5.44.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/edge-craft.git"
  },
  "keywords": [
    "rts",
    "webgl",
    "babylon.js",
    "game-engine",
    "typescript",
    "react"
  ],
  "author": "Vasilisa Versus",
  "license": "MIT"
}
